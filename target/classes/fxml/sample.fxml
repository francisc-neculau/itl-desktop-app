<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.RadioMenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<AnchorPane fx:id="root" stylesheets="@../stylesheet/root.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.itl.desktop.app.controller.MainController">
   <children>
      <HBox fx:id="hbox" prefHeight="27.0" prefWidth="602.0" stylesheets="@../stylesheet/header/toolbar.css">
         <children>
            <MenuBar fx:id="MyMenuBar" stylesheets="@../stylesheet/header/menu_bar.css">
              <menus>
                <Menu mnemonicParsing="false" text="Options">
                  <items>
                        <RadioMenuItem mnemonicParsing="false" text="Override ICR AI" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <Region prefHeight="25.0" prefWidth="500.0" />
            <Button fx:id="closeButton" mnemonicParsing="false" onAction="#handleOnAction" stylesheets="@../stylesheet/header/close_button.css" text="X" />
         </children>
      </HBox>
       <ImageView fx:id="latexImageView" fitHeight="231.0" fitWidth="602.0" layoutY="28.0" onDragDropped="#handleOnDragDropped" onDragOver="#handleOnDragOver" pickOnBounds="true" preserveRatio="true" />
       <TextArea fx:id="latexTextArea" layoutX="1.0" layoutY="260.0" prefHeight="141.0" prefWidth="600.0" />
   </children>
</AnchorPane>
